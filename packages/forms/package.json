{
  "name": "@astro-utils/forms",
  "version": "0.0.0",
  "description": "ASPX like web forms for astro.js",
  "type": "module",
  "scripts": {
    "build": "rm -r dist; tsc",
    "prepack": "npm run build",
    "release": "semantic-release --extends ../../.config/semantic-release.json"
  },
  "keywords": [
    "ASPX",
    "astro",
    "forms",
    "react",
    "hooks"
  ],
  "homepage": "https://withastro-utils.github.io/docs/",
  "author": "Ido S.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/withastro-utils/forms.git"
  },
  "bugs": {
    "url": "https://github.com/withastro-utils/forms/issues"
  },
  "publishConfig": {
